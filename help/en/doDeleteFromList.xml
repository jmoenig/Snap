<help-screen version="1">
    <thumbnail>
        <script>
            <block s="doDeleteFromList">
                <l/>
                <l/>
            </block>
        </script>
    </thumbnail>
    <box color="gray">
        <column>
            <diagram>
                <script>
                    <block s="bubble" annotation="1">
                        <block s="reportListItem">
                            <l><option>last</option></l>
                            <block var="supplies"/>
                        </block>
                        <l>2</l>
                    </block>
                    <block s="doDeleteFromList" annotation="2">
                        <l><option>last</option></l>
                        <block var="supplies"/>
                    </block>
                </script>
                <annotations>
                    <small-p>Says the last item from the list</small-p>
                    <small-p>Deletes the last item from the list</small-p>
                </annotations>
            </diagram>
            <row>
                <column rel-width="1">
                    <p>Before:</p>
                    <img src="doDeleteFromList_supplies_before.png" width="105" height="106"/>
                </column>
                <column rel-width="1">
                    <p>After:</p>
                    <img src="doDeleteFromList_supplies_after.png" width="105" height="93"/>
                </column>
            </row>
        </column>
    </box>
    <box color="blue">
        <column>
            <p>
                You can specify the number of the item you want to delete.
                For example:
            </p>
            <script>
                <block s="doDeleteFromList">
                    <l>2</l>
                    <block var="my list"/>
                </block>
            </script>
            <p>
                To delete the last item in the list, choose "last" from the
                dropdown menu:
            </p>
            <script>
                <block s="doDeleteFromList">
                    <l><option>last</option></l>
                    <block var="my list"/>
                </block>
            </script>
            <p>
                You can also choose to delete everything in the list:
            </p>
            <script>
                <block s="doDeleteFromList">
                    <l><option>all</option></l>
                    <block var="my list"/>
                </block>
            </script>
        </column>
    </box>
</help-screen>
