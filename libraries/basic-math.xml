<blocks app="Snap! 4.0, http://snap.berkeley.edu" version="1">
	<block-definition category="operators" s="%'base' ^ %'exponent'" type="reporter">
		<header/>
		<code/>
		<inputs>
			<input type="%n"/>
			<input type="%n"/>
		</inputs>
		<script>
			<block s="doReport">
				<block s="evaluate">
					<block s="reportJSFunction">
						<list>
							<l>a</l>
							<l>b</l>
						</list>
						<l>return Math.pow(a,b);</l>
					</block>
					<list>
						<block var="base"/>
						<block var="exponent"/>
					</list>
				</block>
			</block>
		</script>
	</block-definition>
</blocks>